  "scripts": {
    "build": "rm -rf dist && tsup",
    "dev": "tsup --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "echo \"Library build test\" && npm run build && echo \"Build successful!\"",
    "check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@react-hook/merged-ref": "^1.3.2",
