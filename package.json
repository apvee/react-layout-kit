    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.3.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "emotion",
    "responsive",
    "ui"
