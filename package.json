      "import": "./dist/components/AreaGrid/index.mjs",
      "require": "./dist/components/AreaGrid/index.js"
    },
    "./components/AspectRatio": {
      "types": "./dist/components/AspectRatio/index.d.ts",
      "import": "./dist/components/AspectRatio/index.mjs",
      "require": "./dist/components/AspectRatio/index.js"
