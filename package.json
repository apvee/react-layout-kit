{
  "name": "@apvee/react-layout-kit",
  "version": "0.0.1",
  "description": "An opinionated, type-safe React layout library that simplifies styling and layout creation without leaving the React component context. Features responsive CSS-in-JS with Box, Grid, Flex, and Stack components.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "llm-full.txt"
  ],
  "typesVersions": {
    "*": {
      "components/*": ["./dist/components/*.d.ts"],
      "core/*": ["./dist/core/*.d.ts"],
      "types": ["./dist/types.d.ts"],
      "hooks": ["./dist/hooks.d.ts"]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./components/Box": {
      "types": "./dist/components/Box.d.ts",
      "import": "./dist/components/Box.mjs",
      "require": "./dist/components/Box.js"
    },
    "./components/AreaGrid": {
      "types": "./dist/components/AreaGrid.d.ts",
      "import": "./dist/components/AreaGrid.mjs",
      "require": "./dist/components/AreaGrid.js"
    },
    "./components/AspectRatio": {
      "types": "./dist/components/AspectRatio.d.ts",
      "import": "./dist/components/AspectRatio.mjs",
      "require": "./dist/components/AspectRatio.js"
    },
    "./components/Center": {
      "types": "./dist/components/Center.d.ts",
      "import": "./dist/components/Center.mjs",
      "require": "./dist/components/Center.js"
    },
    "./components/Container": {
      "types": "./dist/components/Container.d.ts",
      "import": "./dist/components/Container.mjs",
      "require": "./dist/components/Container.js"
    },
    "./components/Flex": {
      "types": "./dist/components/Flex.d.ts",
      "import": "./dist/components/Flex.mjs",
      "require": "./dist/components/Flex.js"
    },
    "./components/Grid": {
      "types": "./dist/components/Grid.d.ts",
      "import": "./dist/components/Grid.mjs",
      "require": "./dist/components/Grid.js"
    },
    "./components/Stack": {
      "types": "./dist/components/Stack.d.ts",
      "import": "./dist/components/Stack.mjs",
      "require": "./dist/components/Stack.js"
    },
    "./components/Group": {
      "types": "./dist/components/Group.d.ts",
      "import": "./dist/components/Group.mjs",
      "require": "./dist/components/Group.js"
    },
    "./components/SimpleGrid": {
      "types": "./dist/components/SimpleGrid.d.ts",
      "import": "./dist/components/SimpleGrid.mjs",
      "require": "./dist/components/SimpleGrid.js"
    },
    "./components/Space": {
      "types": "./dist/components/Space.d.ts",
      "import": "./dist/components/Space.mjs",
      "require": "./dist/components/Space.js"
    },
    "./components/ScrollArea": {
      "types": "./dist/components/ScrollArea.d.ts",
      "import": "./dist/components/ScrollArea.mjs",
      "require": "./dist/components/ScrollArea.js"
    },
    "./core/components": {
      "types": "./dist/core/components.d.ts",
      "import": "./dist/core/components.mjs",
      "require": "./dist/core/components.js"
    },
    "./core/responsive": {
      "types": "./dist/core/responsive.d.ts",
      "import": "./dist/core/responsive.mjs",
      "require": "./dist/core/responsive.js"
    },
    "./core/styling": {
      "types": "./dist/core/styling.d.ts",
      "import": "./dist/core/styling.mjs",
      "require": "./dist/core/styling.js"
    },
    "./core/configuration": {
      "types": "./dist/core/configuration.d.ts",
      "import": "./dist/core/configuration.mjs",
      "require": "./dist/core/configuration.js"
    },
    "./core/utils": {
      "types": "./dist/core/utils.d.ts",
      "import": "./dist/core/utils.mjs",
      "require": "./dist/core/utils.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsup",
    "dev": "tsup --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "echo \"Library build test\" && npm run build && echo \"Build successful!\"",
    "check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@react-hook/merged-ref": "^1.3.2",
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@types/node": "^24.5.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.3.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "emotion",
    "responsive",
    "ui"
  ],
  "homepage": "https://github.com/apvee/react-layout-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apvee/react-layout-kit.git"
  },
  "bugs": {
    "url": "https://github.com/apvee/react-layout-kit/issues"
  },
  "author": "Apvee Solutions",
  "license": "MIT"
}
